{
  "name": "@counterfactual/dev-utils",
  "version": "0.0.2",
  "engines": {
    "node": ">=6.12"
  },
  "description": "Counterfactual dev TS utilities",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "shx rm -rf lib",
    "lint:fix": "tslint --project --fix",
    "lint": "tslint --project .",
    "precommit": "lint-staged"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/counterfactual/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/counterfactual/monorepo/issues"
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "yarn lint:fix",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@counterfactual/configs": "0.0.1",
    "@counterfactual/typescript-typings": "0.0.1",
    "@types/bignumber.js": "4.0.2",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.9.3",
    "jest": "^23.6.0",
    "lint-staged": "7.1.2",
    "shx": "^0.3.2",
    "truffle": "4.1.14",
    "tslint": "5.11.0",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "bignumber.js": "7.2.1",
    "ethers": "^4.0.4"
  }
}
