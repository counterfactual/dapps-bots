{
  "name": "@counterfactual/playground",
  "version": "0.0.1",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "yarn test:spec",
    "test:spec": "stencil test --spec",
    "test:e2e": "echo 'E2E tests are disabled, skipping'",
    "test:watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@counterfactual/node": "^0.0.1",
    "@stencil/core": "0.16.0",
    "@stencil/router": "~0.2.6",
    "@stencil/sass": "^0.1.1",
    "eventemitter3": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/counterfactual/monorepo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/counterfactual/monorepo"
  },
  "homepage": "https://counterfactual.com",
  "devDependencies": {
    "@counterfactual/playground-server": "^0.0.1",
    "@counterfactual/typescript-typings": "^0.0.1",
    "@types/puppeteer": "1.11.1",
    "puppeteer": "1.11.0",
    "rollup-plugin-replace": "^2.1.0"
  }
}
