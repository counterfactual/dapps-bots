/**
 * WARNING: This file is generated by combining the root level tsconfig with the
 *          package level tsconfig. Ideally it should be auto-generated. It is necessary
 *          because on Heroku we do not have access to the root level tsconfig.
 */
{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017", "dom", "esnext.asynciterable"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noLib": false,
    "noUnusedLocals": true,
    "outDir": "dist",
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es2017",
    "typeRoots": ["./node_modules/@counterfactual/typescript-typings/types"],
    "types": ["jsonwebtoken", "knex", "koa", "koa__cors", "uuid", "jest"]
  }
}
