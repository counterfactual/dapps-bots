{
  "name": "@counterfactual/greenboard",
  "version": "0.0.1",
  "description": "An automated end-to-end test suite for Counterfactual, built with Selenium.",
  "keywords": [
    "counterfactual",
    "e2e",
    "end-to-end",
    "tests",
    "selenium"
  ],
  "author": "Joel Alejandro Villarreal Bertoldi <joel.a.villarreal@gmail.com>",
  "homepage": "https://github.com/counterfactual/monorepo#readme",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/counterfactual/monorepo.git"
  },
  "scripts": {
    "start": "jest"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": false
  },
  "bugs": {
    "url": "https://github.com/counterfactual/monorepo/issues"
  },
  "devDependencies": {
    "@types/chromedriver": "^2.38.0",
    "@types/jest": "^24.0.15",
    "@types/selenium-webdriver": "^4.0.1",
    "chromedriver": "2.38",
    "jest": "^24.8.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {}
}
